{
  "notify": false,
  "extensions": {
    "js": {
      "name": "babel-core/register",
      "options": {
        "presets": [
          "react"
        ],
        "plugins": [
          "transform-class-properties",
          "transform-object-rest-spread",
          "dynamic-import-node"
        ]
      }
    }
  }
}
